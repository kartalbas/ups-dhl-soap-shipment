    <xs:complexType>
      <xs:sequence>
        <xs:element ref="bcs:Version">
          <xs:annotation>
            <xs:documentation>The version of the webservice implementation.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Status" type="bcs:Statusinformation">
          <xs:annotation>
