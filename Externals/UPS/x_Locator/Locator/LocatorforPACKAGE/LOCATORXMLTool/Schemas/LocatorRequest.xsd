<xsd:schema elementFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ups="http://www.ups.com/XMLSchema">  <xsd:element name="LocatorRequest">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Request"/>        <xsd:element minOccurs="0" name="OriginAddress" type="OriginAddressType"/>        <xsd:element name="Translate" type="TranslateType"/>        <xsd:element minOccurs="0" name="UnitOfMeasurement" type="UnitOfMeasurementType"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="LocationID" type="xsd:string"/>        <xsd:element minOccurs="0" name="LocationSearchCriteria" type="LocationSearchCriteriaType"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="GeocodeType">    <xsd:sequence>      <xsd:element name="Latitude" type="xsd:string"/>      <xsd:element name="Longitude" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="OriginAddressType">    <xsd:sequence>      <xsd:element minOccurs="0" name="LandmarkCode" type="xsd:string" ups:usage="notused"/>      <xsd:element minOccurs="0" name="Geocode" type="GeocodeType"/>      <xsd:element minOccurs="0" name="AddressKeyFormat" type="AddressKeyFormatType"/>      <xsd:element minOccurs="0" name="MaximumListSize" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="AddressKeyFormatType">    <xsd:sequence>      <xsd:element minOccurs="0" name="ConsigneeName" type="xsd:string" ups:usage="notused"/>      <xsd:element minOccurs="0" name="AddressLine" type="xsd:string"/>      <xsd:element minOccurs="0" name="PoliticalDivision3" type="xsd:string"/>      <xsd:element minOccurs="0" name="PoliticalDivision2" type="xsd:string"/>      <xsd:element minOccurs="0" name="PoliticalDivision1" type="xsd:string"/>      <xsd:element minOccurs="0" name="PostcodePrimaryLow" type="xsd:string"/>      <xsd:element minOccurs="0" name="PostcodeExtendedLow" type="xsd:string"/>      <xsd:element minOccurs="0" name="CountryCode" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="TranslateType">    <xsd:sequence>      <xsd:element name="LanguageCode" type="xsd:string"/>      <xsd:element minOccurs="0" name="Code" type="xsd:string" ups:usage="notused"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="UnitOfMeasurementType">    <xsd:sequence>      <xsd:element name="Code" type="xsd:string"/>      <xsd:element minOccurs="0" name="Description" type="xsd:string" ups:usage="notused"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="LocationSearchCriteriaType">    <xsd:sequence>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SearchOption" type="SearchOptionType"/>      <xsd:element minOccurs="0" name="MaximumListSize" type="xsd:string"/>      <xsd:element minOccurs="0" name="SearchRadius" type="xsd:string"/>      <xsd:element minOccurs="0" name="ServiceSearch" type="ServiceSearchType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="SearchOptionType">    <xsd:sequence>      <xsd:element name="OptionType" type="CodeType"/>      <xsd:element maxOccurs="unbounded" name="OptionCode" type="OptionCodeType"/>      <xsd:element minOccurs="0" name="Relation" type="CodeType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="OptionCodeType">    <xsd:sequence>      <xsd:element name="Code" type="xsd:string"/>      <xsd:element minOccurs="0" name="Description" type="xsd:string" ups:usage="notused"/>      <xsd:element minOccurs="0" name="Name" type="xsd:string" ups:usage="notused"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="CodeType">    <xsd:sequence>      <xsd:element name="Code" type="xsd:string"/>      <xsd:element minOccurs="0" name="Description" type="xsd:string" ups:usage="notused"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="ServiceSearchType">    <xsd:sequence>      <xsd:element minOccurs="0" name="Time" type="xsd:string"/>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ServiceCode" type="CodeType"/>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ServiceOptionCode" type="CodeType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="MapDimensionsType">    <xsd:sequence>      <xsd:element name="Height" type="xsd:string"/>      <xsd:element name="Width" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="ImageMapType">    <xsd:sequence>      <xsd:element name="LocationID" type="xsd:string"/>      <xsd:element name="XCoordinate" type="xsd:string"/>      <xsd:element name="YCoordinate" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:element name="Request">    <xsd:complexType>      <xsd:sequence>        <xsd:element minOccurs="0" ref="TransactionReference"/>        <xsd:element name="RequestAction" type="xsd:string"/>        <xsd:element name="RequestOption" type="xsd:string"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="TransactionReference">    <xsd:complexType>      <xsd:sequence>        <xsd:element minOccurs="0" name="CustomerContext" type="xsd:string"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element></xsd:schema>