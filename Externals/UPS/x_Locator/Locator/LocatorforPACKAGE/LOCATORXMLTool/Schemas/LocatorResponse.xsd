<xsd:schema elementFormDefault="unqualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ups="http://www.ups.com/XMLSchema">  <xsd:element name="LocatorResponse">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Response"/>        <xsd:element minOccurs="0" name="Geocode" type="GeocodeType"/>        <xsd:element minOccurs="0" name="SearchResults" type="SearchResultsType"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="GeocodeType">    <xsd:sequence>      <xsd:element name="Latitude" type="xsd:string"/>      <xsd:element name="Longitude" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="SearchResultsType">    <xsd:sequence>      <xsd:choice>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="GeocodeCandidate" type="GeocodeCandidateType"/>        <xsd:sequence maxOccurs="unbounded" minOccurs="0">          <xsd:element minOccurs="0" name="Disclaimer" type="xsd:string"/>          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DropLocation" type="DropLocationType"/>        </xsd:sequence>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="AvailableLocationAttributes" type="LocationAttributesType"/>      </xsd:choice>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="GeocodeCandidateType">    <xsd:sequence>      <xsd:element name="AddressKeyFormat" type="AddressKeyFormatType"/>      <xsd:element name="Geocode" type="GeocodeType"/>      <xsd:element minOccurs="0" name="LandmarkName" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="AddressKeyFormatType">    <xsd:sequence>      <xsd:element minOccurs="0" name="ConsigneeName" type="xsd:string"/>      <xsd:element minOccurs="0" name="AddressLine" type="xsd:string"/>      <xsd:element minOccurs="0" name="PoliticalDivision3" type="xsd:string"/>      <xsd:element minOccurs="0" name="PoliticalDivision2" type="xsd:string"/>