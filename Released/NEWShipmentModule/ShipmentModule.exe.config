  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ShipmentServiceSOAP11Binding">
          <security mode="Transport" />
        </binding>
        <binding name="ShipmentServiceSOAP11Binding1" />
      </basicHttpBinding>
      <customBinding>
        <binding name="ShipmentServiceSOAP12Binding">
          <textMessageEncoding messageVersion="Soap12" />
          <httpsTransport />
        </binding>
      </customBinding>
    </bindings>
    <client>
      <endpoint address="https://cig.dhl.de/services/production/soap"
          binding="basicHttpBinding"
		  bindingConfiguration="ShipmentServiceSOAP11Binding"
          contract="ShipWebReference.ISWSServicePortType"
		  name="ShipmentServiceSOAP11port0" />
      <endpoint address="https://cig.dhl.de/services/production/soap"
          binding="customBinding"
		  bindingConfiguration="ShipmentServiceSOAP12Binding"
          contract="ShipWebReference.ISWSServicePortType"
		  name="ShipmentServiceSOAP12port0" />
    </client>
	
