  <xs:complexType name="ShipmentDetailsTDType">
    <xs:annotation>
      <xs:documentation>Details of a TD shipment.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="is:ShipmentDetailsType">
        <xs:sequence>
          <xs:annotation>
            <xs:documentation>extends the ShipmentDetailsType
						</xs:documentation>
          </xs:annotation>
          <xs:element name="Account">
            <xs:annotation>
              <xs:documentation>Container element to provide DHL Express Account Number that is needed to invoke Express shipments. 
							</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element ref="cis:accountNumberExpress" />
              </xs:sequence>
            </xs:complexType>
