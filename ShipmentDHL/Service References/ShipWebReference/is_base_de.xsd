  <xs:complexType name="ShipmentItemTDType">
    <xs:annotation>
      <xs:documentation>Item/Piece data of a TD shipment.
			</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="is:ShipmentItemType">
        <xs:sequence>
          <xs:annotation>
            <xs:documentation>extends the ShipmentItemType
						</xs:documentation>
          </xs:annotation>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ShipmentItemDDType">
    <xs:annotation>
      <xs:documentation>Item/Piece data of a DD shipment.
			</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="is:ShipmentItemType">
        <xs:sequence>
          <xs:annotation>
            <xs:documentation>extends the ShipmentItemType
						</xs:documentation>
          </xs:annotation>
          <xs:element name="PackageType" type="xs:string">
            <xs:annotation>
              <xs:documentation><![CDATA[
						<context id="CreateShipmentDDRequest">
							PackageType is always "PK"! It might be varied only for product code EUP. Field length must be less than or equal to 5.
							<values>
								<value>
										<name>CO</name>
										<description>Palett</description>
								</value>
								<value>
										<name>PK</name>
										<description>Paket</description>
								</value>
				</values>
						</context>
					]]></xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ShipmentServiceTD">
    <xs:annotation>
      <xs:documentation>TD shipment services.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:annotation>
          <xs:documentation>can be</xs:documentation>
        </xs:annotation>
        <xs:element name="ServiceGroupDateTimeOption" type="is:TDServiceGroupDateTimeOptionType">
          <xs:annotation>
            <xs:documentation>Service bundle that only contains one service. Follwing service must be chosen, if the service bundle is invoked.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ServiceGroupOther" type="is:TDServiceGroupOtherType">
          <xs:annotation>
            <xs:documentation>Service bundle that only contains one service to be booked. Following service must be chosen, if the service bundle is invoked.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TDServiceGroupDateTimeOptionType">
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>Service Group Date / Time Option.
				</xs:documentation>
      </xs:annotation>
      <xs:element name="ExpressSaturday">
        <xs:annotation>
          <xs:documentation>Container element must be set to provide exact shipping date that must be a Saturday.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ShippingDate">
              <xs:annotation>
                <xs:documentation>If ExpressSaturday is set, shipping date must be passed in format yyyy-mm-dd to provide exact date (Saturday) where delivery shall occur.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength value="10" />
                  <xs:maxLength value="10" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TDServiceGroupOtherType">
    <xs:annotation>
      <xs:documentation>Other TD Services.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="Insurance">
          <xs:annotation>
            <xs:documentation>Container element must be set to provide insurance value.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="InsuranceAmount">
                <xs:annotation>
                  <xs:documentation>If Insurance is set, insurance amount must be passed. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:decimal">
                    <xs:minInclusive value="0.0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="InsuranceCurrency">
                <xs:annotation>
                  <xs:documentation>If Insurance is set, insurance currency must also be passed. Field length must be = 3.
									</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="GoGreen" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>GoGreen Service.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ShipmentServiceDD">
    <xs:annotation>
      <xs:documentation>DD shipment services.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:annotation>
          <xs:documentation>can be</xs:documentation>
        </xs:annotation>
        <xs:element name="ServiceGroupDateTimeOption" type="is:DDServiceGroupDateTimeOptionType">
          <xs:annotation>
            <xs:documentation>Bundles following services for domestic time-definite products offered by DHL Express. One of the services must be chosen, if this service bundle is invoked.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ShipmentServiceGroupIdent" type="is:DDShipmentServiceGroupIdentType">
          <xs:annotation>
            <xs:documentation>Bundles following ident services.One of the services must be chosen, if this service bundle is invoked.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ShipmentServiceGroupPickup" type="is:DDShipmentServiceGroupPickupType">
          <xs:annotation>
            <xs:documentation>One of the services must be chosen, if this service bundle is invoked. Bundles following services. </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ServiceGroupBusinessPackInternational" type="is:DDServiceGroupBusinessPackInternationalType">
          <xs:annotation>
            <xs:documentation>Service group for Business Pack International (product code BPI). One of the services must be chosen, if this service bundle is invoked. Bundles following services.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ServiceGroupDHLPaket" type="is:DDServiceGroupDHLPaketType">
          <xs:annotation>
            <xs:documentation>Service group for DHL Paket (product code EPN and RPN). One of the services must be chosen, if this service bundle is invoked. Bundles following services.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ServiceGroupOther" type="is:DDServiceGroupOtherType">
          <xs:annotation>
            <xs:documentation>One of the services must be chosen, if this service bundle is invoked. Service group bundles following services.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DDServiceGroupDateTimeOptionType">
    <xs:annotation>
      <xs:documentation>Service Group Date Time Option. 
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="DeliveryOnTime">
          <xs:annotation>
            <xs:documentation>Service to deliver on particular time requires to specify desired time in child node. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="time" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Desired time of delivery, format is 'hh:mm'(mandatory if DeliveryOnTime is defined). Field length must be = 4.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="DeliveryEarly" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Early delivery.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Express0900" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Express 09:00</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Express1000" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Express 10:00</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Express1200" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Express 12:00</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DeliveryAfternoon" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Afternoon delivery.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DeliveryEvening" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Evening delivery.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ExpressSaturday" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Express Saturday.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ExpressSunday" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Express Sunday.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DDShipmentServiceGroupIdentType">
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>Service Group Ident.</xs:documentation>
      </xs:annotation>
      <xs:choice>
        <xs:element name="Ident">
          <xs:annotation>
            <xs:documentation>Service to validate identity of recipient. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Name" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Name (firstname + lastname). Mandatory if service Ident is chosen. Field length must be less than or equal to 55.
									</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DateOfBirth">
                <xs:annotation>
                  <xs:documentation>Date of birth in format yyyy-mm-dd. 
Mandatory if service Ident is chosen. 
									</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="10" />
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="IdentityCardType" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The type of the identity card (e.g. ID, Passport).
Mandatory if service Ident is chosen.
									</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="IdentityCardNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The number of the identity card.
Mandatory if service Ident is chosen.
									</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="IdentExtra">
          <xs:annotation>
            <xs:documentation>Service to validate identity by means of two documents that can be of either DrivingLicense, IdentityCard, BankCard. The element ShipmentOrder.Shipment.Identity is mandatory, if IdentExtra is used. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Group1" type="is:IdentityData">
                <xs:annotation>
                  <xs:documentation>The first one of two potential identity documents is selected in Group1. </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Group2" type="is:IdentityData">
                <xs:annotation>
                  <xs:documentation>The second of two potential identity proofs is selected in Group2. Note: it cannot be the same instrument as the one chosen in Group1.
Mandatory if service IdentExtra is chosen.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="IdentPremium">
          <xs:annotation>
            <xs:documentation>Ident Premium</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="Firstname" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The firstname.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="Name" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The surname.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="IdentityCardType" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The type of the identity card: valid values are "01"=(Identitycard/Personalausweis), "02"=Passport.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="IdentityCardNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The number of the identity card. Field length must be less than or equal to 20.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="MinimumAge" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Minimum age (16 or 18). Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="DateOfBirth">
                <xs:annotation>
                  <xs:documentation>Date of birth in format yyyy-mm-dd. Field length must be less than or equal to 35.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="10" />
                    <xs:maxLength value="10" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="StreetAndHouseNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Streetname as printed on the identity card.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="PostcodeAndCity" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Postcode and city as printed on the identity card.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="District" type="xs:string">
                <xs:annotation>
                  <xs:documentation>District of the city.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="WithIDPContract" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>With IDP Contract.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="ContractID" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Constract ID.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="Nationality" type="xs:string">
                <xs:annotation>
                  <xs:documentation>iso contrycode (e.g. DE, NL, US)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="FreeText1" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Freetext 1</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="FreeText2" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Freetext 2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfNameAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of name is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfFirstNameAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of first name is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfDayOfBirthAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of day of birth is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfMinimumAgeAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of minimum age is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfIdentityCardTypeAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the identity card type is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfIdentityCardNumberAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the identity card number is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfAddressAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the address is allowed (postcode, city, district, streetname, housenumber)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfContractAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the contract is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfContractIdAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the IDP contract ID is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfNationalityAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the nationality is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="CorrOfFreetextsAllowed" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Flag, if the correction of the freetexts is allowed</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Personally" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Invoke service personal handover.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ReturnReceipt" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Invoke service return receipt.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ProofOfDelivery" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Invoke service proof of delivery.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ContractSubmission">
          <xs:annotation>
            <xs:documentation>Invoke service contract submission. Field length = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TotalPages">
                <xs:annotation>
                  <xs:documentation>Total number of pages. 
Checksum for deliverer. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:minInclusive value="0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TotalSignatures">
                <xs:annotation>
                  <xs:documentation>Total number of signatures. 
Checksum for deliverer. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:minInclusive value="0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TotalDocsSender">
                <xs:annotation>
                  <xs:documentation>Total number of docs to be sent back to sender.
Checksum for deliverer. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:minInclusive value="0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="TotalDocsReceiver">
                <xs:annotation>
                  <xs:documentation>Total number of docs to remain at receiver. 
Checksum for deliverer. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:minInclusive value="0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SMSAviso">
          <xs:annotation>
            <xs:documentation>Invoke service SMSAviso to inform customers about date and time of delivery via SMS. Field length must be = 1. </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="MobilePhoneNumberSender" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The mobile phone number of the sender. Mandatory if service SMSAviso is chosen. Note: must not contain any 
special characters! Field length must be less than or equal to 20.

									</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MobilePhoneNumberReceiver" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The mobile phone number of the receiver. Mandatory if service SMSAviso is chosen. Note: must not contain any 
special characters! Field length must be less than or equal to 20.

									</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="AvisoIdent" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The self generated aviso ident code that is sent to receiver along with SMS for authentication and shipment association purpose. Field length must be less than or equal to 11.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="CheckMinimumAge">
          <xs:annotation>
            <xs:documentation>Service to validate minimum age of recipient. Field max length must be = 2.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="MinimumAge" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The minimum age.
Mandatory if service CheckMinimumAge is chosen. Field length must be less than or equal to 2 characters.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DDShipmentServiceGroupPickupType">
    <xs:annotation>
      <xs:documentation>Pickup services.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>Service Group Pickup.</xs:documentation>
      </xs:annotation>
      <xs:choice>
        <xs:element name="PickupSaturday" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Pickup on Saturday.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PickupLate" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Pickup Late.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DDServiceGroupBusinessPackInternationalType">
    <xs:annotation>
      <xs:documentation>Service Group BPI.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="Economy" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Service Economy.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Premium" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Premium for fast and safe delivery of international shipments.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Seapacket" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Seapacket.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="CoilWithoutHelp" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Coil without help.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Endorsement">
          <xs:annotation>
            <xs:documentation>Service endorsement is used to specify handling if recipient not met. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Ident" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Obsolete field that is currently not in use. </xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Days" type="xs:integer">
                <xs:annotation>
                  <xs:documentation>Specifies number of days that shipment will be held in business days before returned to sender.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="AmountInternational" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Service to utilize higher insurance amount up to 25.000 EUR depending on delivery country.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DDServiceGroupDHLPaketType">
    <xs:annotation>
      <xs:documentation>Service Group DHL Paket.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="Multipack" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Must be set to create EPN shipments with more than one shipment item. Special customer clearance required.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="RegioPacket" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Service MUST be set with product code RPN regio paket to AT.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ParticularDelivery" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>For EPN shipments with multiple items, this flag determines whether items will be delivered alltogether (=FALSE) or whether items can be delivery partially (=TRUE). Note: if COD is chosen as service, flag must be FALSE. 
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="ShipmentAdvisory">
          <xs:annotation>
            <xs:documentation>Service "SendungsAvise".</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="999" name="AdvisoryData">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="ModuleType" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The module type. Valid values are A1, A2, B1.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="MobilePhoneNumber" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The mobile phone number of the receiver.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="EmailAddress" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The email address of the receiver.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Reference" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Reference.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Language" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>ISO CountryCode of the language. Valid values are "EN", "DE".</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Endorsement">
          <xs:annotation>
            <xs:documentation>Service "Vorausverfgung". Only valid for EPN.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="UZN" type="is:EndorsementServiceconfiguration">
                <xs:annotation>
                  <xs:documentation>Option Unzustellbarkeitsnachricht (UZN)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="KeNa" type="is:EndorsementServiceconfiguration">
                <xs:annotation>
                  <xs:documentation>Option Keine Nachbarschaftszustellung (KeNa)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="NSI" type="is:EndorsementServiceconfiguration">
                <xs:annotation>
                  <xs:documentation>Option Nachsendeinformation (NSI)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="TeZu" type="is:EndorsementServiceconfigurationTeZu">
                <xs:annotation>
                  <xs:documentation>Option Terminzustellung (TeZu)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="SoZue" type="is:EndorsementServiceconfiguration">
                <xs:annotation>
                  <xs:documentation>Option Sofort zurck (SoZue)</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EndorsementServiceconfiguration">
    <xs:attribute name="active" use="required">
      <xs:annotation>
        <xs:documentation>Indicates, if the option is on/off</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="0" />
          <xs:enumeration value="1" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="EndorsementServiceconfigurationTeZu">
    <xs:attribute name="active" use="required">
      <xs:annotation>
        <xs:documentation>Indicates, if the option is on/off</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="0" />
          <xs:enumeration value="1" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="tezuDate" use="required">
      <xs:annotation>
        <xs:documentation>Date of option Terminzustellung (TeZu) is mandatory, if the option is used: Date in format yyyy-mm-dd</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="10" />
          <xs:maxLength value="10" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="DDServiceGroupOtherType">
    <xs:annotation>
      <xs:documentation>Other DD Services.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="HigherInsurance">
          <xs:annotation>
            <xs:documentation>Insure shipment with higher than standard amount. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="InsuranceAmount">
                <xs:annotation>
                  <xs:documentation>To book either one of the two insurance options field must contain either "2500" or "25000" exactly.
Mandatory if HigherInsurance is chosen.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:decimal">
                    <xs:minInclusive value="0.0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="InsuranceCurrency" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Currency of insurance.
Mandatory if HigherInsurance is chosen.
									</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="COD">
          <xs:annotation>
            <xs:documentation>Cash on delivery service. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CODAmount">
                <xs:annotation>
                  <xs:documentation>Money amount to be collected.
Mandatory if COD is chosen. Field length must be less than or equal to 22.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:decimal">
                    <xs:minInclusive value="0.0" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="CODCurrency" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Currency of cod amount.
Mandatory if COD is chosen. Field length must be = 3.
									</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Unfree">
          <xs:annotation>
            <xs:documentation>Service unfree, Receiver pays shipment fees. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PaymentType">
                <xs:annotation>
                  <xs:documentation>Mandatory if unfree is chosen. 0= cash / 1= invoice. </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="0" />
                    <xs:enumeration value="1" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CustomerNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Paid by third party, e.g. Receiver. DHL account number mandatory if PaymentType 1 = invoice is selected. Field length must be less than or equal to 10.
</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="DangerousGoods">
          <xs:annotation>
            <xs:documentation>Service to ship hazardous goods. If selected dangerous goods class, packaging type and UN code must be specified. Field length must be = 1.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DangerousGoodsClass" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Dangerous goods classification. Field length must be = 1.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DangerousGoodsPackagingType" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Dangerous packaging type.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DangerousGoodsUNCode" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Dangerous goods UN code.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Bulkfreight">
          <xs:annotation>
            <xs:documentation>Service to ship bulkfreight items. Bulkfreight type can be selected optionally. Field length must be = 1.
						</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="BulkfreightType">
                <xs:annotation>
                  <xs:documentation>Bulkfreight type. Mandatory if bulkfreight is selected. Field length must be less than or equal to 20.
									</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="Lang" />
                    <xs:enumeration value="L" />
                    <xs:enumeration value="XL" />
                    <xs:enumeration value="XXL" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="DirectInjection" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Direct Injection Service.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Bypass" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>Bypass Service.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ShipmentNotificationType">
    <xs:annotation>
      <xs:documentation>Notification type</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RecipientName" type="xs:string">
        <xs:annotation>
          <xs:documentation>Name of the email recipient. Mandatory if Notification is set. Field length must be less than or equal to 45.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecipientEmailAddress" type="xs:string">
        <xs:annotation>
          <xs:documentation>Email address of the recipient. Mandatory if Notification is set. Field length must be less than or equal to 20.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExportDocumentTDType">
    <xs:annotation>
      <xs:documentation>The data of the export document for a TD
				shipment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InvoiceType">
        <xs:annotation>
          <xs:documentation>Type of invoice. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="proforma" />
            <xs:enumeration value="commercial" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="InvoiceDate">
        <xs:annotation>
          <xs:documentation>Invoice date in format yyyy-mm-dd.
Mandatory if Export Document is provided. Field length must be = 8.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10" />
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="InvoiceNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>In case invoice has a number, client app can provide it in this field. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ExportType">
        <xs:annotation>
          <xs:documentation>Type of export. ("P"=permanent / "T"=temporary / "R"=repair and return). Field length must be less than or equal to 40.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="P" />
            <xs:enumeration value="T" />
            <xs:enumeration value="R" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="SignerTitle" type="xs:string">
        <xs:annotation>
          <xs:documentation>The title of the Signer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Remark" type="xs:string">
        <xs:annotation>
          <xs:documentation>Freely editable field to leave a remark.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="CommodityCode" type="xs:string">
        <xs:annotation>
          <xs:documentation>For trading internationally, the standardized international commodity codes for shipped goods are specified. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ExportReason" type="xs:string">
        <xs:annotation>
          <xs:documentation>Reason for exporting goods.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="999" name="ExportDocPosition">
        <xs:annotation>
          <xs:documentation>More than one position only possible for international TD shipments. One or more child elements for every position to be defined within the Export Document.
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Description" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the single position. Field length must be less than or equal to 40.
								</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Amount">
              <xs:annotation>
                <xs:documentation>Defines the amount of pieces of that respective position. Field length must be less than or equal to 22.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ValuePerPiece">
              <xs:annotation>
                <xs:documentation>Defines the value of every piece of the position. Field length must be less than or equal to 22.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="NetWeightInKG">
              <xs:annotation>
                <xs:documentation>Net weight of the shipment. Field length must be less than or equal to 22.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="GrossWeightInKG">
              <xs:annotation>
                <xs:documentation>Gross weight of the shipment. Field length must be less than or equal to 22.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ISOCountryCodeOfOrigin">
              <xs:annotation>
                <xs:documentation>Defines the ISO code of origin country. Field length must be = 2.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:maxLength value="2" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="CommodityCode" type="xs:string">
              <xs:annotation>
                <xs:documentation>For trading internationally, the standardized international commodity codes for shipped goods are specified. Field length must be less than or equal to 30.
								</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExportDocumentDDType">
    <xs:annotation>
      <xs:documentation>The data of the export document for a DD
				shipment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="InvoiceType">
        <xs:annotation>
          <xs:documentation>Invoice type (only mandatory for BPI). Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="proforma" />
            <xs:enumeration value="commercial" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="InvoiceDate">
        <xs:annotation>
          <xs:documentation>Invoice date in format yyyy-mm-dd.
Mandatory if Export Document is provided. Field length must be = 8.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10" />
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="InvoiceNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>In case invoice has a number, client app can provide it in this field. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ExportType">
        <xs:annotation>
          <xs:documentation>Export type ("0"="other", "1"="gift",
						"2"="sample", "3"="documents", "4"="goods return") (depends on
						chosen product -&gt; only mandatory for BPI). Field length must be less than or equal to 40.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="0" />
            <xs:enumeration value="1" />
            <xs:enumeration value="2" />
            <xs:enumeration value="3" />
            <xs:enumeration value="4" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="ExportTypeDescription" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description mandatory if ExportType is 'other' (= 0). Field length must be less than or equal to 30.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="CommodityCode" type="xs:string">
        <xs:annotation>
          <xs:documentation>For trading internationally, the standardized international commodity code for shipped good is specified. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TermsOfTrade" type="xs:string">
        <xs:annotation>
          <xs:documentation>Element provides terms of trades, i.e. incoterms codes like DDU, CIP et al. Field length must be = 3.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount">
        <xs:annotation>
          <xs:documentation>Amount of shipment positions. Multiple positions not allowed for EUP and EPI, only BPI allows amount &gt; 1. Field length must be less than or equal to 22.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Description" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description text for the document.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryCodeOrigin" type="xs:string">
        <xs:annotation>
          <xs:documentation>Defines the ISO  code of origin country. Field length must be = 2.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="AdditionalFee">
        <xs:annotation>
          <xs:documentation>Additional custom fees to be payed.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CustomsValue">
        <xs:annotation>
          <xs:documentation>Defines the declared customs value amount of the shipment. Field length must be less than or equal to 11.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CustomsCurrency" type="xs:string">
        <xs:annotation>
          <xs:documentation>Sets the declared customs value currency of the shipment. Field length must be = 3.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="PermitNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>The permit number. Field length must be less than or equal to 30.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="AttestationNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>The attestation number. Field length must be less than or equal to 30.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="WithElectronicExportNtfctn" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Sets an electronic export notification. 
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="MRNNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>The MRN number. Field length must be equal to 30.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="999" name="ExportDocPosition">
        <xs:annotation>
          <xs:documentation>One or more child elements for every position to be defined within the Export Document. Each one contains description, amount, value, net and gross weight, country code of origin et al. Multiple positions only possible for BPI, but not for EUP and EPI.
					</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Description" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the respective position. Field length must be less than or equal to 40.
								</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="CountryCodeOrigin" type="xs:string">
              <xs:annotation>
                <xs:documentation>Defines the ISO code of origin country. 
								</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="CommodityCode" type="xs:string">
              <xs:annotation>
                <xs:documentation>Standardized international commodity code for respective position is specified. Field length must be less than or equal to 30.
								</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Amount">
              <xs:annotation>
                <xs:documentation>Defines the amount of pieces of that respective position. Minimum amount is 1. Field length must be less than or equal to 22.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:integer">
                  <xs:minInclusive value="1" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="NetWeightInKG">
              <xs:annotation>
                <xs:documentation>Net weight of the shipment. Field length must be less than or equal to 22.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="GrossWeightInKG">
              <xs:annotation>
                <xs:documentation>Gross weight of the shipment. Field length must be less than or equal to 22.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="CustomsValue">
              <xs:annotation>
                <xs:documentation>Defines the declared customs value amount of respective position. Field length must be less than or equal to 11.
								</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="0.0" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="CustomsCurrency" type="xs:string">
              <xs:annotation>
                <xs:documentation>Sets the declared customs value currency of respective position.
								</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FurtherAddressesDDType">
    <xs:annotation>
      <xs:documentation><![CDATA[Further address information
The following data fields from the cis_base-types are processed/mandatory/optional:
-------------------------------------------------------------------------------------------------------
Salutation		 (optional) : cis:NameType->Person->salutation
Company Name 1   (mandatory): cis:NameType->Company->name1
Company Name 2   (optional) : cis:NameType->Company->name2
Contact Name     (mandatory): cis:CommunicationType->contactPerson
Street Name      (mandatory): cis:NativeAddressType->streetName
Street Number    (mandatory): cis:NativeAddressType->streetNumber
Add. Address     (optional) : cis:NativeAddressType->careOfName
Postcode         (mandatory): cis:NativeAddressType->zip
City Name		 (mandatory): cis:NativeAddressType->city
ISO Country Code (mandatory): cis:NativeAddressType->Origin->CountryType->countryISOType 
Phone Number     (mandatory): cis:CommunicationType->phone
Email Address    (mandatory): cis:CommunicationType->email						
			]]></xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="DeliveryAdress">
        <xs:annotation>
          <xs:documentation>Mandatory if further address is to be specified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Company" type="cis:NameType">
              <xs:annotation>
                <xs:documentation>Determines whether further address is one of the following types.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="Name3" type="xs:string">
              <xs:annotation>
                <xs:documentation>Extra data for name extension.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="cis:NativeAddressType">
              <xs:annotation>
                <xs:documentation>Contains address data.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Communication" type="cis:CommunicationType">
              <xs:annotation>
                <xs:documentation>Info about communication.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LabelData">
    <xs:annotation>
      <xs:documentation>The status of the getLabel operation and the url
				for requesting the label (if available).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ShipmentNumber" type="cis:ShipmentNumberType">
        <xs:annotation>
          <xs:documentation>For successful and unsuccessful operations, the requested ShipmentNumber of the label to be retrieved is returned. This is no matter if it the operation could be performed or not. Depending on the invoked product (TD or DD) </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="is:Statusinformation">
        <xs:annotation>
          <xs:documentation>Success status of processing retrieval of particular shipment label.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Labelurl" type="xs:string">
        <xs:annotation>
          <xs:documentation>If label output format was requested to be 'URL' in initial ShipmentOrder of the CreateShipment call, Labelurl will be returned and include the label as URL pointing to a PDF label.
Note that the output format (URL or XML) is determined by the initial ShipmentOrder and cannot be set in GetLabel request.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="XMLLabel" type="xs:string">
        <xs:annotation>
          <xs:documentation>If label output format was requested to be 'XML' in initial ShipmentOrder of the CreateShipment call, XMLLabel will be returned and include the label as XML data in a CDATA-stream.  
Note that the output format (URL vs XML) is determined by the initial ShipmentOrder call and cannot be set in GetLabel request.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ExportDocData">
    <xs:annotation>
      <xs:documentation>The status of the getLabel operation and the url for requesting the label (if available)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ShipmentNumber" type="cis:ShipmentNumberType">
        <xs:annotation>
          <xs:documentation>ShipmentNumber</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="is:Statusinformation">
        <xs:annotation>
          <xs:documentation>Status of the request (value of zero means, the request was processed without error; value greater than zero indicates that an error occurred).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element minOccurs="0" name="ExportDocPDFData" type="xs:string">
          <xs:annotation>
            <xs:documentation>Export doc as base64 encoded pdf data</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ExportDocURL" type="xs:string">
          <xs:annotation>
            <xs:documentation>URL for downloading the Export doc as pdf</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ManifestState">
    <xs:annotation>
      <xs:documentation>The status of a doManifest operation.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ShipmentNumber" type="cis:ShipmentNumberType">
        <xs:annotation>
          <xs:documentation>ShipmentNumber</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="is:Statusinformation">
        <xs:annotation>
          <xs:documentation>Status of the request (value of zero means,
						the request was processed without error; value greater than zero
						indicates that an error occurred).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeletionState">
    <xs:annotation>
      <xs:documentation>The status of a deleteShipment operation.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ShipmentNumber" type="cis:ShipmentNumberType">
        <xs:annotation>
          <xs:documentation>For successful and unsuccessful operations, the requested ShipmentNumber to be deleted is returned. This is no matter if the operation could be performed or not. Depending on the invoked product (TD or DD).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="is:Statusinformation">
        <xs:annotation>
          <xs:documentation>Success status of processing the deletion of particular shipment.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="BookPickupRequest">
    <xs:annotation>
      <xs:documentation>The data for a pickup order.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cis:Version">
          <xs:annotation>
            <xs:documentation>The version of the webservice implementation
							for which the requesting client is developed.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="BookingInformation" type="is:PickupBookingInformationType">
          <xs:annotation>
            <xs:documentation>Contains information in further leaf elements about product, DHL account, pickup date and time, pickup location, amount of pieces, pallets, and shipments, moreover weight and volume weight, size. 
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PickupAddress" type="is:PickupAddressType">
          <xs:annotation>
            <xs:documentation>The pickup address.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ContactOrderer" type="is:PickupOrdererType">
          <xs:annotation>
            <xs:documentation>The address and contact information of the
							orderer.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="BookPickupResponse">
    <xs:annotation>
      <xs:documentation>The data for a pickup order.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cis:Version">
          <xs:annotation>
            <xs:documentation>The version of the webservice implementation.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Status" type="is:Statusinformation">
          <xs:annotation>
            <xs:documentation>Success status after processing the request.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ConfirmationNumber" type="xs:string">
          <xs:annotation>
            <xs:documentation>The confirmation number of the successfully created pickup order. It can later be used for cancelling a pickup order. Confirmation number is not available for each pickup type.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ShipmentNumber" type="xs:string">
          <xs:annotation>
            <xs:documentation>If available, a shipment number is returned.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="PickupDetailsType">
    <xs:annotation>
      <xs:documentation>The details of a pickup order.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PickupDate">
        <xs:annotation>
          <xs:documentation>Pickup date in format yyyy-mm-dd.
Mandatory if pickup is booked along with shipment order.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10" />
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ReadyByTime">
        <xs:annotation>
          <xs:documentation>Earliest time for pickup. 
Format is hh:mm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5" />
            <xs:maxLength value="5" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ClosingTime">
        <xs:annotation>
          <xs:documentation>Lates time for pickup. 
Format is hh:mm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5" />
            <xs:maxLength value="5" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="Remark" type="xs:string">
        <xs:annotation>
          <xs:documentation>Remarks to be considered when pickup is done.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PickupLocation" type="xs:string">
        <xs:annotation>
          <xs:documentation>Area to further detail pickup location beyond address.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PickupAddressType">
    <xs:annotation>
      <xs:documentation><![CDATA[
The pickup address. In the PickupType the following data fields are processed/mandatory/optional:
----------------------------------------------------------------------------------------------
Company Name 1   (mandatory): cis:NameType->Company->name1
Contact Name     (mandatory): cis:CommunicationType->contactPerson
Street Name      (mandatory): cis:NativeAddressType->streetName
Street Number    (mandatory): cis:NativeAddressType->streetNumber
Add. Address 	 (optional) : cis:NativeAddressType->careOfName
Postcode         (mandatory): cis:NativeAddressType->zip
City Name		 (mandatory): cis:NativeAddressType->city
ISO Country Code (mandatory): cis:NativeAddressType->Origin->CountryType->countryISOType 
Phone Number     (mandatory): cis:CommunicationType->phone
Email Address    (mandatory): cis:CommunicationType->email
			]]></xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Company" type="cis:NameType">
        <xs:annotation>
          <xs:documentation>Determines whether pickup address is one of the following types.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address" type="cis:NativeAddressType">
        <xs:annotation>
          <xs:documentation>Data fields for pickup address.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Communication" type="cis:CommunicationType">
        <xs:annotation>
          <xs:documentation>Info about communication.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PickupOrdererType">
    <xs:annotation>
      <xs:documentation><![CDATA[
Information about the pickup orderer. In the PickupOrdererType the following data fields are processed/mandatory/optional:
----------------------------------------------------------------------------------------------
Salutation		 (optional) : cis:NameType->Person->salutation
Company Name 1   (mandatory): cis:NameType->Company->name1
Company Name 2   (optional) : cis:NameType->Company->name2
Contact Name     (mandatory): cis:CommunicationType->contactPerson
Street Name      (mandatory): cis:NativeAddressType->streetName
Street Number    (mandatory): cis:NativeAddressType->streetNumber
Add. Address     (optional) : cis:NativeAddressType->careOfName
Postcode         (mandatory): cis:NativeAddressType->zip
City Name		 (mandatory): cis:NativeAddressType->city
ISO Country Code (mandatory): cis:NativeAddressType->Origin->CountryType->countryISOType 
Phone Number     (mandatory): cis:CommunicationType->phone
Email Address    (mandatory): cis:CommunicationType->email		
				]]></xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Company" type="cis:NameType">
        <xs:annotation>
          <xs:documentation>Determines whether orderer contact address is of the following type.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Name3" type="xs:string">
        <xs:annotation>
          <xs:documentation>Optional name appendix.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Address" type="cis:NativeAddressType">
        <xs:annotation>
          <xs:documentation>Data fields for orderer's address.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Communication" type="cis:CommunicationType">
        <xs:annotation>
          <xs:documentation>Info about communication.
					</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PickupBookingInformationType">
    <xs:annotation>
      <xs:documentation>The data of the pickup order.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProductID">
        <xs:annotation>
          <xs:documentation>Information about the shipment to be picked up. ("TDN" = time-definite national / "TDI" = time-definite international / "DDN" = day-definite national / "DDI" = day-definite international). </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="TDI" />
            <xs:enumeration value="TDN" />
            <xs:enumeration value="DDI" />
            <xs:enumeration value="DDN" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Account" type="xs:string">
        <xs:annotation>
          <xs:documentation>Depending on whether a DD pickup or TD pickup is invoked, this field contains either the 10-digit EKP number (DD pickups) or the 9-digit accountNumberExpress (TD pickups).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Attendance" type="xs:string">
        <xs:annotation>
          <xs:documentation>Field has the partner id. I.e. the last 2 digit number extract from the 14 digit DHL Account Number. E.g. if DHL Account Number is "5000000008 72 01" then Attendance is 01. Field length must be = 2. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PickupDate">
        <xs:annotation>
          <xs:documentation>Pickup date in format yyyy-mm-dd.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="10" />
            <xs:maxLength value="10" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ReadyByTime">
        <xs:annotation>
          <xs:documentation>Earliest time  for pickup. 
Format is hh:mm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5" />
            <xs:maxLength value="5" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ClosingTime">
        <xs:annotation>
          <xs:documentation>Lates time for pickup. 
Format is hh:mm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="5" />
            <xs:maxLength value="5" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="Remark" type="xs:string">
        <xs:annotation>
          <xs:documentation>Remarks to be considered when pickup is done.
Mandatory if 'TDI' is selected.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="PickupLocation" type="xs:string">
        <xs:annotation>
          <xs:documentation>Area to further detail pickup location beyond address. Mandatory for TDN and TDI, optional for DDN and DDI.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="AmountOfPieces">
        <xs:annotation>
          <xs:documentation>Number of pieces to be picked up.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="AmountOfPallets">
        <xs:annotation>
          <xs:documentation>Number of pallets to be picked up.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="WeightInKG">
        <xs:annotation>
          <xs:documentation>The weight of all shipment's pieces in kg. Field length must be less than or equal to 22.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="CountOfShipments">
        <xs:annotation>
          <xs:documentation>Number of shipments to be picked up.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:minInclusive value="0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="TotalVolumeWeight">
        <xs:annotation>
          <xs:documentation>The total volumetric weight of all pieces in kg. 
Calculated by piece =
length x width x height in centimetres / 5000. Field length must be less than or equal to 22.
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="MaxLengthInCM">
        <xs:annotation>
          <xs:documentation>The maximum length in cm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="MaxWidthInCM">
        <xs:annotation>
          <xs:documentation>The maximum width in cm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="MaxHeightInCM">
        <xs:annotation>
          <xs:documentation>The maximum height in cm.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="CancelPickupRequest">
    <xs:annotation>
      <xs:documentation>The data for cancelling a pickup order.
			</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cis:Version">
          <xs:annotation>
            <xs:documentation>The version of the webservice implementation
							for which the requesting client is developed.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="BookingConfirmationNumber" type="xs:string">
          <xs:annotation>
            <xs:documentation>The confirmation number of the pickup order which should be cancelled. Use value from pickup response attribute 'ConfirmationNumber' to cancel respective pickup order.
Note: only one pickup can be deleted at a time.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
