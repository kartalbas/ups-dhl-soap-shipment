    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="cis:Version">
          <xs:annotation>
            <xs:documentation>The version of the webservice implementation.
						</xs:documentation>
          </xs:annotation>
        </xs:element>
